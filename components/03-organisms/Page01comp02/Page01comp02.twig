<section id="featured-services" class="featured-services section">
  <div class="container">
    <div class="row gy-4">
      {% for molecule in molecules %}
        <div class="col-lg-3 col-md-6 aos-init aos-animate" data-aos="fade-up" data-aos-delay="{{ loop.index * 100 }}">
          <div class="service-item {{ molecule.header_class }} position-relative">
            {% include "@molecules/CardMolecule/CardMolecule.twig" with {
              icon_class: molecule.icon_class,
              card_text: molecule.card_text,
              header_class: molecule.header_class,
              card_url: molecule.card_url,
              para_text: molecule.para_text,
              para_class: molecule.para_class
            } %}
          </div>
        </div>
      {% endfor %}
    </div>
  </div>
</section>
